{
    "name": "planner-data",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "lint": "npx eslint --config ./config/.eslintrc.cjs ./src/**/*",
        "lint:fix": "npx eslint --fix --config ./config/.eslintrc.cjs ./src/**/*",
        "format:check": "npx eslint-config-prettier config/.eslintrc.cjs",
        "format": "npx prettier --write ./src/**/* --config ./config/.prettierrc.cjs",
        "style": "pnpm lint:fix && pnpm format",
        "build": "tsc --project ./config/build.tsconfig.json",
        "start": "node --experimental-specifier-resolution=node ./dist/index.js",
        "re": "pnpm build && pnpm start",
        "git": "pnpm style && pnpm re",
        "open": "explorer.exe .\\data\\raw",
        "analyze": "tsc --project ./config/analysis.tsconfig.json && node --experimental-specifier-resolution=node ./dist/analysis/index.js",
        "analyze:js": "node --experimental-specifier-resolution=node ./data/analysis.js"
    },
    "type": "module",
    "author": "Jeuk Hwang",
    "license": "ISC",
    "dependencies": {
        "@tsconfig/node16-strictest-esm": "^1.0.3",
        "@types/better-sqlite3": "^7.6.3",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "better-sqlite3": "^8.0.1",
        "eslint": "^8.26.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "exceljs": "^4.3.0",
        "prettier": "^2.7.1",
        "sqlite3": "^5.1.2",
        "typescript": "^4.8.4"
    }
}